<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>MonadicResponseHandler</PackageId>
    <Version>1.1.0</Version>
    <Authors>Gustavo Murayama</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/gmurayama/monadic-response-handler</RepositoryUrl>
    <Description>Minimal library that allows a more declarative programming style for handling function results based on two possible states: Ok and Err.</Description>
    <PackageTags>C# Functional Monad</PackageTags>
  </PropertyGroup>

</Project>
